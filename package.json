    "@remix-run/node": "^2.5.0",
    "@remix-run/react": "^2.5.0",
    "@remix-run/serve": "^2.5.0",
    "@sanity/asset-utils": "^1.3.0",
    "@sanity/image-url": "^1.0.2",
    "@sentry/remix": "^7.93.0",
    "@tailwindcss/typography": "^0.5.10",
    "clsx": "^2.1.0",
    "isbot": "^4.1.0",
    "picosanity": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.5.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
